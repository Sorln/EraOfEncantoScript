local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Era Of Encanto Script   (By: Sorln#3037)", "Ocean")
local Tab = Window:NewTab("Gifts")
local Section = Tab:NewSection("Gifts Of Encanto")
Section:NewButton("Healing Light", "ButtonInfo", function()
    print("Clicked")
    local ohString1 = "Healing Gift"
local ohInstance2 = workspace.CasitaNature.Tree.MeshPart
local ohCFrame3 = CFrame.new(149.912918, -20.0567322, 75.3039169, 0.536023557, -0.0152920047, -0.844064593, 9.31322575e-10, 0.999836087, -0.0181141291, 0.844203115, 0.00970959943, 0.535935581)

game:GetService("ReplicatedStorage").Events.Power:FireServer(ohString1, ohInstance2, ohCFrame3)
end)
